{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "outDir" : "compile"
  },
  "exclude" : [
    "node_modules",
    "**/main.aot.ts"
  ],
  "angularCompilerOptions": {
    "genDir" : "aot",
    "skipMetadataEmit" : true
  }
}
